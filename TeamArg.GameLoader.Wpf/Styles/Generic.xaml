<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    
                    xmlns:local="clr-namespace:TeamArg.GameLoader.Wpf.Styles">

    <SolidColorBrush x:Key="ControlBackgroundBrush" Color="{DynamicResource Green}"/>
    <SolidColorBrush x:Key="HeaderBackgroundBrush" Color="{DynamicResource Coffee}"/>
    <SolidColorBrush x:Key="FooterBackgroundBrush" Color="{DynamicResource Footer}"/>
    <SolidColorBrush x:Key="ForegroundBrush" Color="{DynamicResource Foreground}"/>
    <SolidColorBrush x:Key="LightForegroundBrush" Color="White"/>
</ResourceDictionary>